
@model List<Employee>

<div class="container mt-5">
    <h2>Person List</h2>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>ID</th>
                <th>First Name</th>
                <th>Last Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var employee in Model)
            {
                <tr>
                    <td>@employee.Id</td>
                    <td> @employee.Name </td>
                    <td>@employee.LastName</td>
                </tr>
                
            }
        </tbody>
    </table>
</div>
